<html>
  <head></head>
  <body>
  <form action="/" method="get">
  <a href='/'>Click here to sign out</a>
  </form>
    <h1>Peeps</h1>
    <a href="/new_peep/<%= session[:user_id] %>"><button type="button"">Post a new peep</button></a>

    <% @peep_repo.all.reverse.each do |peep| %>
      <div>
      <p><%= @user_repo.find(peep.user_id).username %>: <%= peep.content %></p> 
      <a href="/comment/<%= peep.id %>">Comment</a>
      <p><%= peep.time_posted %>
    </div>
    <% end %>
  </body>
</html>